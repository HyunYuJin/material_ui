{
  "name": "material_ui",
  "version": "0.0.1",
  "description": "MUI",
  "main": "dist/Mui.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack serve --progress --env dev",
    "deploy": "npm run build && npm publish",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HyunYuJin/material_ui.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Hyun Yu Jin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HyunYuJin/material_ui/issues"
  },
  "homepage": "https://github.com/HyunYuJin/material_ui#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "worker-loader": "^3.0.8",
    "yargs": "^14.0.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "ignore": [
      "/config",
      "/demo",
      "/docs",
      "/src/utils.js"
    ]
  }
}